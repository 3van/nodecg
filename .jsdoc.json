{
	"source": {
		"include": [
			"./lib",
			"./README.md"
		],
		"exclude": [
			"./lib/browser/dist",
			"./lib/browser/shims",
			"./lib/browser/replicant",
			"./lib/replicant/shared",
			"./lib/replicator",
			"./lib/bundle-manager",
			"./lib/logger"
		]
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": [
			"jsdoc"
		]
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true
	},
	"opts": {
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "./node_modules/minami",
		"tutorials": "./tutorials",
		"destination": "./docs/"
	},
	"markdown": {
		"idInHeadings": true
	}
}
